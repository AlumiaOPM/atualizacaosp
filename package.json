{
  "name": "react-serverless-auth-example",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "description": "An example implementation of react-serverless-auth React Components",
  "keywords": [
    "react",
    "serverless",
    "auth",
    "example",
    "amazon",
    "aws",
    "cognito"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stanchino/react-serverless-auth/example"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.3.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "framer-motion": "^3.10.0",
    "history": "^4.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-gtm-module": "^2.0.11",
    "react-icons": "^4.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "build": "react-scripts build"
  },
  "devDependencies": {
    "@chakra-ui/icons": "^1.0.10",
    "babel-core": "^6.26.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/shared-examples.{js,jsx}",
      "!src/*.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
